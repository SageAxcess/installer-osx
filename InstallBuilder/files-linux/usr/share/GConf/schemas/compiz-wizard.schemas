      <type>int</type>
      <default>3000</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Hard limit for particles</short>
        <long>If this hard limit is reached, no new particles are created. (100 - 5000)</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/wizard/screen0/options/soft_limit</key>
      <applyto>/apps/compiz-1/plugins/wizard/screen0/options/soft_limit</applyto>
      <owner>compiz-1</owner>
      <type>int</type>
      <default>2000</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Soft limit for particles</short>
