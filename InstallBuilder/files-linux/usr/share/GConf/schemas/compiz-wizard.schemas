      <type>list</type>
      <list_type>int</list_type>
      <default>[0,0,0,0,10,0,0,0,0,0]</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Gravity Probability</short>
        <long>The probability that a particle has the gravity effect, that has been set up above. This value represents gravity particles out of 10000 particles. (Using gravity on too many particles may be very resource consuming.)</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/wizard/screen0/options/hard_limit</key>
      <applyto>/apps/compiz-1/plugins/wizard/screen0/options/hard_limit</applyto>
