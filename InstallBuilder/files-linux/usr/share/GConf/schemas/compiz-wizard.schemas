      <type>list</type>
      <list_type>int</list_type>
      <default>[0,0,0,0,200,0,0,0,0,0]</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Gravity Range</short>
        <long>The strength range of the gravity effect, these particles have. (See gravity probability. May be especially resource consuming.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/wizard/screen0/options/e_gp</key>
