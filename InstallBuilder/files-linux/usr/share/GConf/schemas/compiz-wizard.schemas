      <key>/schemas/apps/compiz-1/plugins/wizard/screen0/options/darken</key>
      <applyto>/apps/compiz-1/plugins/wizard/screen0/options/darken</applyto>
      <owner>compiz-1</owner>
      <type>float</type>
      <default>0.9</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Darken background</short>
        <long>Darken background under particles (0.0 - 1.0)</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/wizard/screen0/options/blend</key>
