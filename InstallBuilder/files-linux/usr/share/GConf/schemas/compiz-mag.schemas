      <locale name="C">
        <short>Magnifier image</short>
        <long>Magnifier image.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/mag/screen0/options/mask</key>
      <applyto>/apps/compiz-1/plugins/mag/screen0/options/mask</applyto>
