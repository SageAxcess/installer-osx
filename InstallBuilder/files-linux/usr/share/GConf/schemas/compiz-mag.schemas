      <default>&lt;Super&gt;&lt;Shift&gt;Button4</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Zoom In</short>
        <long>Zoom In</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/mag/screen0/options/zoom_out_button</key>
      <applyto>/apps/compiz-1/plugins/mag/screen0/options/zoom_out_button</applyto>
      <owner>compiz-1</owner>
      <type>string</type>
      <default>&lt;Super&gt;&lt;Shift&gt;Button5</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Zoom Out</short>
        <long>Zoom Out</long>
      </locale>
    </schema>

