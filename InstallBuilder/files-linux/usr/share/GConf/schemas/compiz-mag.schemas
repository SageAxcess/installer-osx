      <key>/schemas/apps/compiz-1/plugins/mag/screen0/options/radius</key>
      <applyto>/apps/compiz-1/plugins/mag/screen0/options/radius</applyto>
      <owner>compiz-1</owner>
      <type>int</type>
      <default>200</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Radius</short>
