      <locale name="C">
        <short>Initiate</short>
        <long>Toggle the mouse pointer trail.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/initiate_button</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/initiate_button</applyto>
      <owner>compiz-1</owner>
      <type>string</type>
      <default>Disabled</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Initiate</short>
        <long>Toggle the mouse pointer trail.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/initiate_edge</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/initiate_edge</applyto>
      <owner>compiz-1</owner>
      <type>string</type>
      <default/>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Initiate</short>
        <long>Toggle the mouse pointer trail.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/rotation_speed</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/rotation_speed</applyto>
