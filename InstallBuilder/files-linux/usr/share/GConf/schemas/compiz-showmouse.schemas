      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/darken</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/darken</applyto>
      <owner>compiz-1</owner>
      <type>float</type>
      <default>0.9</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Darken backgound</short>
        <long>Darken background under particles (0.0 - 1.0)</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/blend</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/blend</applyto>
      <owner>compiz-1</owner>
      <type>bool</type>
      <default>true</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Additive blending</short>
        <long>Additive blending of particles</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/color</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/color</applyto>
      <owner>compiz-1</owner>
      <type>string</type>
      <default>#ffdf3fff</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Particle Color</short>
        <long>Particle color.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/random</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/random</applyto>
      <owner>compiz-1</owner>
      <type>bool</type>
      <default>false</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Randomly Colored Particles</short>
        <long>Have random colors for the particles</long>
      </locale>
