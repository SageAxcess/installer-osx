      <default>3</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Emitters</short>
        <long>Number of particle emitters. (1 - 10)</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/num_particles</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/num_particles</applyto>
