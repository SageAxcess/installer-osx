      <type>float</type>
      <default>0.5</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Rotation speed</short>
        <long>Rotation speed. (0.1 - 10.0)</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/radius</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/radius</applyto>
      <owner>compiz-1</owner>
      <type>int</type>
      <default>100</default>
      <gettext_domain>compiz</gettext_domain>
      <locale name="C">
        <short>Radius</short>
        <long>Ring radius (10 - 1000)</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/showmouse/screen0/options/emitters</key>
      <applyto>/apps/compiz-1/plugins/showmouse/screen0/options/emitters</applyto>
